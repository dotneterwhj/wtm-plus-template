<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageType>Template</PackageType>
    <PackageVersion>1.0.0</PackageVersion>
    <PackageId>Abner.Wtm.Plus</PackageId>
    <Title>Abner.Wtm.Plus</Title>
    <Authors>abner</Authors>
    <Description>Templates to use when creating an application for Wtm.</Description>
    <PackageTags>dotnet-new;templates;contoso</PackageTags>
    <PackageProjectUrl>https://gitea.nailao.space:20443/opensource/wtm-plus-template</PackageProjectUrl>
		<RepositoryUrl>https://gitea.nailao.space:20443/opensource/wtm-plus-template</RepositoryUrl>
    <TargetFramework>net6.0</TargetFramework>

    <IncludeContentInPack>true</IncludeContentInPack>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <ContentTargetFolders>content</ContentTargetFolders>
    <NoDefaultExcludes>true</NoDefaultExcludes>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="template\**\*" Exclude="template\**\bin\**;mytemplate\**\obj\**" />
    <Compile Remove="**\*" />
  </ItemGroup>

</Project>
